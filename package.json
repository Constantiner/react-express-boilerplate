{
	"name": "@constantiner/react-express-boilerplate",
	"version": "0.1.0",
	"bin": "bin/www",
	"description": "A boilerplate for full stack React/Express project",
	"license": "MIT",
	"author": "Konstantin Kovalev <constantiner@gmail.com>",
	"contributors": [
		"Konstantin Kovalev <constantiner@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Constantiner/react-express-boilerplate.git"
	},
	"keywords": [
		"JavaScript",
		"Node",
		"Node.js",
		"React",
		"Express",
		"Full stack"
	],
	"bugs": {
		"url": "https://github.com/Constantiner/react-express-boilerplate/issues"
	},
	"homepage": "https://github.com/Constantiner/react-express-boilerplate#readme",
	"dependencies": {
		"cookie-parser": "^1.4.3",
		"dotenv": "6.0.0",
		"dotenv-expand": "4.2.0",
		"express": "^4.16.4",
		"http-errors": "^1.7.1",
		"log4js": "^3.0.6"
	},
	"devDependencies": {
		"@babel/core": "7.1.0",
		"@constantiner/resolve-node-configs-hierarchy": "^0.1.2",
		"@svgr/webpack": "2.4.1",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "9.0.0",
		"babel-jest": "23.6.0",
		"babel-loader": "8.0.4",
		"babel-plugin-named-asset-import": "^0.2.2",
		"babel-preset-react-app": "^5.0.4",
		"bfj": "6.1.1",
		"case-sensitive-paths-webpack-plugin": "2.1.2",
		"chalk": "2.4.1",
		"css-loader": "1.0.0",
		"eslint": "^5.7.0",
		"eslint-config-prettier": "^3.1.0",
		"eslint-config-react-app": "^3.0.4",
		"eslint-loader": "2.1.1",
		"eslint-plugin-flowtype": "2.50.1",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jsx-a11y": "6.1.1",
		"eslint-plugin-node": "^7.0.1",
		"eslint-plugin-react": "^7.11.1",
		"eslint-plugin-security": "^1.4.0",
		"esm": "^3.0.84",
		"file-loader": "2.0.0",
		"fs-extra": "7.0.0",
		"html-webpack-plugin": "4.0.0-alpha.2",
		"identity-obj-proxy": "3.0.0",
		"jest": "23.6.0",
		"jest-pnp-resolver": "1.0.1",
		"jest-resolve": "23.6.0",
		"mini-css-extract-plugin": "0.4.3",
		"nodemon": "^1.18.4",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"pnp-webpack-plugin": "1.1.0",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.0.6",
		"postcss-safe-parser": "4.0.1",
		"react": "^16.5.2",
		"react-app-polyfill": "^0.1.3",
		"react-dev-utils": "^6.0.5",
		"react-dom": "^16.5.2",
		"resolve": "1.8.1",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.0",
		"terser-webpack-plugin": "1.1.0",
		"url-loader": "1.1.1",
		"webpack": "4.19.1",
		"webpack-dev-server": "3.1.9",
		"webpack-manifest-plugin": "2.0.4",
		"workbox-webpack-plugin": "3.6.2"
	},
	"scripts": {
		"start:client": "node -r esm scripts/client-start.js",
		"build:client": "node -r esm scripts/client-build.js",
		"test:client": "node -r esm scripts/client-test.js --config config/client/jest/jest.config.json",
		"start:server:dev": "NODE_ENV=development node -r esm ./bin/www",
		"start:server": "NODE_ENV=production node -r esm ./bin/www",
		"debug:server": "node -r esm scripts/server-debug.js",
		"lint": "npx eslint '**/**/*.{js,mjs,jsx}'",
		"format": "npx prettier 'server/**/*.{js,mjs,json,jsx}' 'client/**/*.{js,mjs,json,jsx}' 'config/**/*.{js,mjs,json,jsx}' 'scripts/**/*.{js,mjs,json,jsx}' --write"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"prettier": {
		"printWidth": 120,
		"useTabs": true,
		"tabWidth": 4
	},
	"engines": {
		"node": ">=8.5.0",
		"npm": ">=5.2.0"
	},
	"@std/esm": "cjs"
}
